{
  "version": 2,
  "builds": [
    {
      "src": "website/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/lander",
      "dest": "/website/index.html"
    },
    {
      "src": "/app",
      "dest": "/website/app-super-enhanced.html"
    },
    {
      "src": "/privacy",
      "dest": "/website/privacy.html"
    },
    {
      "src": "/terms",
      "dest": "/website/terms.html"
    },
    {
      "src": "/support",
      "dest": "/website/help.html"
    },
    {
      "src": "/pricing",
      "dest": "/website/pricing.html"
    },
    {
      "src": "/about",
      "dest": "/website/about.html"
    },
    {
      "src": "/help",
      "dest": "/website/help.html"
    },
    {
      "src": "/dashboard",
      "dest": "/website/dashboard.html"
    },
    {
      "src": "/library",
      "dest": "/website/library.html"
    },
    {
      "src": "/settings",
      "dest": "/website/settings.html"
    },
    {
      "src": "/auth",
      "dest": "/website/auth.html"
    },
    {
      "src": "/signin",
      "dest": "/website/auth.html"
    },
    {
      "src": "/signup",
      "dest": "/website/auth.html"
    },
    {
      "src": "/profile",
      "dest": "/website/profile.html"
    },
    {
      "src": "/recommendations",
      "dest": "/website/recommendations.html"
    },
    {
      "src": "/audio-editor",
      "dest": "/website/audio-editor.html"
    },
    {
      "src": "/stem-separation",
      "dest": "/website/stem-separation.html"
    },
    {
      "src": "/stems",
      "dest": "/website/stem-separation.html"
    },
    {
      "src": "/ai-studio",
      "dest": "/website/ai-studio.html"
    },
    {
      "src": "/ai",
      "dest": "/website/ai-studio.html"
    },
    {
      "src": "/gamification",
      "dest": "/website/gamification.html"
    },
    {
      "src": "/rewards",
      "dest": "/website/gamification.html"
    },
    {
      "src": "/(.*)",
      "dest": "/website/$1"
    },
    {
      "src": "/",
      "dest": "/website/index.html"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}