{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/create",
      "destination": "/create.html"
    },
    {
      "source": "/library",
      "destination": "/library.html"
    },
    {
      "source": "/remix",
      "destination": "/remix.html"
    },
    {
      "source": "/lyrics",
      "destination": "/lyrics.html"
    },
    {
      "source": "/explore",
      "destination": "/explore.html"
    },
    {
      "source": "/profile",
      "destination": "/profile.html"
    },
    {
      "source": "/pricing",
      "destination": "/pricing.html"
    },
    {
      "source": "/auth",
      "destination": "/auth.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}